    "keywords":    ["force.com", "salesforce"],
    "homepage":    "https://github.com/joshiausdemwald/CodemitteForceToolkitBundle",
    "license":     "MIT",
    "authors": [
        {
            "name": "Johannes Heinen",
            "homepage": "https://github.com/joshiausdemwald"
        }
    ],
    "require": {
        "php" : ">5.3.6",
        "symfony/framework-bundle": ">=2.1,<2.2-dev",
        "codemitte/force-toolkit": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Codemitte\\Bundle\\ForceToolkitBundle": ""
        }
    },
    "target-dir": "Codemitte/Bundle/ForceTookitBundle"
